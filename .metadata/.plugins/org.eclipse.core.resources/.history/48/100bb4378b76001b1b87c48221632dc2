/**
 * 
 */

async function like(image_Id){
	var response = await fetch(`/like/${image_Id}`,{
		method: "POST"
	});
	
	var msg = await response.text();
	return msg;
}


//비동기 처리
async function onFeedLoad(image_Id){
	var msg = await like(image_Id);
	if (msg ==="ok"){
		//.toggleClass 있으면 제거 없으면 추가
		$("#"+imageId).toggleClass("heart-clicked fa-heart fa-heart-o");	
	}else{
		console.log(msg);
	}
}